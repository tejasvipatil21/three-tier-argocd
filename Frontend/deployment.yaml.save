apiVersion: apps/v1
kind: Deployment
metadata:
  name: frontend
  namespace: three-tier
spec:
  replicas: 1
  selector:
    matchLabels:
      app: frontend
  template:
    metadata:
      labels:
        app: frontend
    spec:
      containers:
      - name: frontend
        image: 302263040775.dkr.ecr.ap-south-1.amazonaws.com/frontend:latest
        env:
        - name: BACKEND_URL
          value: "http://backend:3000"
        ports:
        
